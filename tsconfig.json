{
  "extends": "./tsconfig-base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "module": "esnext",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "target": "es5",
    "paths": {
      "*": ["*"],
      "src/*": ["src/*"]
    }
  },
  "include": ["./src"]
}
